{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/_actions(?:/(.*?))?/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/about/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/api/nasa(?:/(.*?))?/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/form/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/game/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/$",
			"dest": "/_isr?x_astro_path=$0"
		}
	]
}